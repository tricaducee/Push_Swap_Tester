#!/bin/zsh

BOLD="\033[1m"

LINE="\033[4m"

ITAL="\033[3m"

PURPLE="\033[35m"

BLUE="\033[34m"

GREEN="\033[32m"

RED="\033[31m"

YELLOW="\033[33m"

CYAN="\033[36m"

RST="\033[0m"

R="\033[41m"

TXT=$BOLD$LINE$ITAL$PURPLE

TXTB=$BOLD$LINE$BLUE

NAME=../push_swap

RET_VAL="Error"

TEMP=0

BIGEST=0

BIGEST_A=0

BIGEST_B=0

BIGEST_C=0

BIGEST_D=0

BIGEST_E=0

I=0

AVERAGE=0

AVERAGE_A=0

AVERAGE_B=0

AVERAGE_C=0

AVERAGE_D=0

AVERAGE_E=0

# echo "\n\033[41m    $RST \033[41m $RST  \033[41m $RST \033[41m    $RST \033[41m $RST  \033[41m $RST   \033[41m    $RST \033[41m $RST     \033[41m $RST   \033[41m  $RST   \033[41m    $RST"
# echo "\033[41m $RST  \033[41m $RST \033[41m $RST  \033[41m $RST \033[41m $RST    \033[41m $RST  \033[41m $RST   \033[41m $RST    \033[41m $RST  \033[41m $RST  \033[41m $RST  \033[41m $RST  \033[41m $RST  \033[41m $RST  \033[41m $RST"
# echo "\033[41m    $RST \033[41m $RST  \033[41m $RST \033[41m    $RST \033[41m    $RST   \033[41m    $RST \033[41m $RST  \033[41m $RST  \033[41m $RST \033[41m      $RST \033[41m    $RST"
# echo "\033[41m $RST    \033[41m $RST  \033[41m $RST    \033[41m $RST \033[41m $RST  \033[41m $RST      \033[41m $RST \033[41m $RST  \033[41m $RST  \033[41m $RST \033[41m $RST    \033[41m $RST \033[41m $RST   "
# echo "\033[41m $RST    \033[41m    $RST \033[41m    $RST \033[41m $RST  \033[41m $RST   \033[41m    $RST  \033[41m  $RST \033[41m  $RST  \033[41m $RST    \033[41m $RST \033[41m $RST   "
# echo "                                                   "
# echo "                   $TXTB TESTER $RST\n"


echo "\n\033[31m██████╗ ██╗   ██╗███████╗██╗  ██╗    ███████╗██╗    ██╗ █████╗ ██████╗ "
echo "██╔══██╗██║   ██║██╔════╝██║  ██║    ██╔════╝██║    ██║██╔══██╗██╔══██╗"
echo "██████╔╝██║   ██║███████╗███████║    ███████╗██║ █╗ ██║███████║██████╔╝"
echo "██╔═══╝ ██║   ██║╚════██║██╔══██║    ╚════██║██║███╗██║██╔══██║██╔═══╝ "
echo "██║     ╚██████╔╝███████║██║  ██║    ███████║╚███╔███╔╝██║  ██║██║     "
echo "╚═╝      ╚═════╝ ╚══════╝╚═╝  ╚═╝    ╚══════╝ ╚══╝╚══╝ ╚═╝  ╚═╝╚═╝     "
echo "                               $TXTB TESTER $RST\n                         "


make -C ..
if [ ! -e ../push_swap ]
then
	echo "\033[31mERROR$RST : $ITAL\033[31mLa compilation à échouée$RST"
	exit 1
fi

if [ ! -e checker ]
then
	make
fi

#--------------------------------------Func----------------------------------------#

average()
{
	TEMP=$((`$NAME $* | wc -l | awk '{print $1}'`))
	AVERAGE=$(($TEMP + $AVERAGE))
	if [ $TEMP -gt $BIGEST ]
	then
		BIGEST=$TEMP
	fi
	I=$((1 + $I))
}

compareResult()
{
	STR=`$NAME $* 2>&1`
	if [[ $STR == $RET_VAL ]]
	then
		echo "${BOLD}${GREEN}[OK]${RST}"
	else
		echo "${BOLD}${RED}[KO]${RST}"
		echo "${RED}Expected value ${RST}: ${ITAL}${RED}Error (with newline)${RST}"
		echo "${RED}Yours ${RST}: ${ITAL}${RED}${STR}${RST}"
	fi
}

compareResultNothing()
{
	STR=`$NAME $* 2>&1`
	if [[ $STR == $RET_VAL ]]
	then
		echo "${BOLD}${GREEN}[OK]${RST}"
	else
		echo "${BOLD}${RED}[KO]${RST}"
		echo "${RED}Expected value ${RST}: ${ITAL}${RED}Nothing${RST}"
		echo "${RED}Yours ${RST}: ${ITAL}${RED}${STR}${RST}"
	fi
}

testfunction2 ()
{
	for i in {1..$1}
	do
		ARG="`ruby -e "puts ($2..$3).to_a.shuffle.join (' ')"`"
		$NAME $ARG | ./checker $ARG -cm
		average $ARG
	done
}

funCompareHighRange()
{
	$NAME $* | ./checker $* -cm
	average $*
}

#                                     ERROR                                         #
#-----------------------------------------------------------------------------------#

echo "${TXT}\nTest d'erreur :${RST}\n"

#-------------------------------------Weird-----------------------------------------#
echo "${ITAL}${PURPLE}\nWeird tests :${RST}"

RET_VAL=""
compareResultNothing " -1 "

RET_VAL="Error"
compareResult " 1-1 "
compareResult " -"
compareResult " -+"
compareResult "1- "
compareResult " - a"
compareResult " '-' qwecs"
compareResult "3 1 2 0 9 17  28891 1291283 31 231823 -"
compareResult "3 1 2 0 9 17  28891 1291283 31 231823 -   "

#-------------------------------------Weird multi-----------------------------------------#
echo "${ITAL}${PURPLE}\nWeird multi tests :${RST}"

compareResult  1-1
compareResult " " -1
compareResult -
compareResult - +
compareResult 1- 
compareResult - a
compareResult  '-' qwecs
compareResult 3 1 2 0 9 17  28891 1291283 31 231823 - 
compareResult 3 1 2 0 9 17  28891 1291283 "31  " 231823 " -   "

