echo "${ITAL}${PURPLE}moins de 11500 ${RST}:${BLUE} 1${RST}\n"

echo "$TXT\nTon average score :$RST\n"
echo "${ITAL}${PURPLE}Pour 3         ${RST}:${BLUE} ${AVERAGE_A}${RST}\n"
echo "${ITAL}${PURPLE}Pour 4         ${RST}:${BLUE} ${AVERAGE_B}${RST}\n"
